              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">// Get current date
def calendar = Calendar.getInstance()

// Get current day, month, and year
def day = calendar.get(Calendar.DAY_OF_MONTH)
def month = calendar.get(Calendar.MONTH) + 1  // Months are 0-based
def year = calendar.get(Calendar.YEAR)

// Generate a random 10-digit mobile number
def random = new Random()
def mobileNumber = &quot;98&quot; + (1000000000 + random.nextInt(900000000)).toString()


// Store these as JMeter variables
vars.put(&quot;signup_day&quot;, day.toString())
vars.put(&quot;signup_month&quot;, month.toString())
vars.put(&quot;signup_year&quot;, year.toString())

// Store this value in a JMeter variable
vars.put(&quot;mobile_number&quot;, mobileNumber)
</stringProp>
