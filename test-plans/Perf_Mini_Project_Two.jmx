              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">// Random name
def firstNames = [&apos;Ram&apos;, &apos;Ravi&apos;, &apos;Raj&apos;, &apos;Kumar&apos;, &apos;Amit&apos;, &apos;Anil&apos;]
def lastNames = [&apos;Singh&apos;, &apos;Sharma&apos;, &apos;Patel&apos;, &apos;Reddy&apos;, &apos;Gupta&apos;]
def nameOnCard = firstNames[new Random().nextInt(firstNames.size())] + &quot; &quot; +
                 lastNames[new Random().nextInt(lastNames.size())]

// Generate card number: 12-16 digit numeric string
def cardNumber = (1..12).collect { (Math.random() * 10).intValue() }.join()

// Generate 3-digit CVC
def cvc = String.format(&apos;%03d&apos;, new Random().nextInt(1000))

// Expiry month and year
def expiryMonth = String.format(&apos;%02d&apos;, 1 + new Random().nextInt(12))
def expiryYear = String.valueOf(2025 + new Random().nextInt(5))  // e.g. 2025–2029

// Set JMeter variables
vars.put(&quot;card_name&quot;, nameOnCard)
vars.put(&quot;card_number&quot;, cardNumber)
vars.put(&quot;card_cvc&quot;, cvc)
vars.put(&quot;expiry_month&quot;, expiryMonth)
vars.put(&quot;expiry_year&quot;, expiryYear)
</stringProp>
            </JSR223PreProcessor>
